{
    "name": "reduxpress",
    "version": "1.4.6",
    "description": "",
    "main": "libs",
    "types": "./@types/index.d.ts",
    "scripts": {
        "start": "nodemon example",
        "lint": "eslint .",
        "prepublish": "npm run docs",
        "docs": "jsdoc2md --template README.hbs libs/**/*.js > README.md",
        "test": "mocha --require test/support/env --reporter spec --bail --check-leaks --no-exit test/**/**.spec.js test/acceptance/",
        "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --require test/support/env --reporter spec --check-leaks --no-exit test/**/**.spec.js test/acceptance/",
        "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --require test/support/env --reporter dot --check-leaks --no-exit test/**/**.spec.js test/acceptance/",
        "test-tap": "mocha --require test/support/env --reporter tap --check-leaks --no-exit test/**/**.spec.js test/acceptance/"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rajatady/Reduxpress.git"
    },
    "author": "Kumar Divya Rajat <rajat.ady@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/rajatady/Reduxpress",
    "dependencies": {
        "async": "^2.5.0",
        "bluebird": "^3.5.3",
        "chalk": "^2.1.0",
        "console.table": "^0.9.1",
        "iplocation": "^6.0.4",
        "jsonwebtoken": "^8.4.0",
        "lodash": "^4.17.10",
        "mongoose": "^5.2.6",
        "querymen": "^2.1.3",
        "request": "^2.87.0",
        "speakeasy": "^2.0.0",
        "@types/express": "^4.17.0",
        "@types/mongoose": "^5.5.6",
        "@types/bluebird": "^3.5.26"
    },
    "devDependencies": {
        "body-parser": "^1.18.3",
        "chai": "^4.1.2",
        "eslint": "4.18.2",
        "express": "^4.16.3",
        "istanbul": "0.4.5",
        "jsdoc-to-markdown": "^4.0.1",
        "mocha": "^5.2.0",
        "should": "13.1.0",
        "sinon": "^5.0.10",
        "supertest": "^3.3.0",
        "ts-lint": "^4.5.1",
        "ts-node": "^8.0.2",
        "tslint": "^5.13.1",
        "typescript": "^3.4.5"
    }
}
